            <kind>1</kind>
            <string>/// Initialize

// ! This is an object to test the Extension, it is not required for the Extension functionality

// initialize the exTimer system, do this on game start before everything else
ex_timer_initialize();

// enable debug messages (check the console)
ex_timer_set_debug_mode(true);

// create a class to group timers into
ex_timer_class_create("all_timers");

// create a new timer and attach a script to it, plus add it to the class we created
ex_timer_create("after100", 100, 1, false, "all_timers", false, scr_timer1_action);

// create a new looping timer with a faster speed and attach a script to it, plus add it to the class we created
ex_timer_create("after200", 200, 5, true, "all_timers", false, scr_timer2_action);

// another after 300
ex_timer_create("after300", 300, 1, false, "all_timers", false, scr_timer3_action);

// another after 400 but synced to delta_time (click on top of titlebar to cause lag)
ex_timer_create("after400_synced", 400, 1, false, "all_timers", true, scr_timer4_action);

// start the timers
ex_timer_play("after100");
//ex_timer_play("after200");
ex_timer_play("after300");
ex_timer_play("after400_synced");

// quick start! create and play a local timer, use a number as "name", this will be auto-cleared when it triggers or at the end of the room
// mimics the GMS alarm[] but can be also synced to delta_time and you can have unlimited number of them
ex_timer_play(0, 500, 1, false, false, scr_timer5_action);

// also start an alarm for comparison
alarm[0] = 300;
</string>
